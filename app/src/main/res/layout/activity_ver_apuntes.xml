<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="verApuntesVM" type="com.application.app.modules.verapuntes.data.viewmodel.VerApuntesVM"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup238"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylelime_100">

        <ScrollView
            android:id="@+id/scrollViewScrollview"
            android:layout_width="412dp"
            android:layout_height="733dp"
            android:layout_marginTop="@dimen/_10pxv"
            android:clipToPadding="false"
            android:fillViewport="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintGroup230"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/imageGroup37"
                    android:layout_width="415dp"
                    android:layout_height="57dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group19"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/txtFsica"
                    style="@style/txtSansitaSwashedromanregular36"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{verApuntesVM.verApuntesModel.txtFsica}"
                    app:layout_constraintBottom_toTopOf="@+id/txtApuntes1fech"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:text="@string/lbl_f_sica" />

                <TextView
                    android:id="@+id/txtApuntes1fech"
                    style="@style/txtSansitaSwashedromanregular18_1"
                    android:layout_width="327dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="28dp"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:singleLine="true"
                    android:text="@{verApuntesVM.verApuntesModel.txtApuntes1fech}"
                    app:layout_constraintBottom_toTopOf="@+id/imageImage1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.439"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:text="@string/msg_apuntes_1_fech" />

                <ImageView
                    android:id="@+id/imagePngkey3"
                    imageRes="@{@drawable/img_pngkey2}"
                    android:layout_width="@dimen/_73pxh"
                    android:layout_height="@dimen/_76pxh"
                    android:layout_marginEnd="16dp"
                    android:scaleType="fitXY"
                    app:layout_constraintBottom_toTopOf="@+id/txtFsica"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.48"
                    tools:src="@drawable/img_pngkey2" />

                <ImageView
                    android:id="@+id/imageMenu"
                    imageRes="@{@drawable/img_menu}"
                    android:layout_width="@dimen/_64pxh"
                    android:layout_height="@dimen/_61pxh"
                    android:scaleType="fitXY"
                    app:layout_constraintBottom_toTopOf="@+id/txtFsica"
                    app:layout_constraintEnd_toStartOf="@+id/imagePngkey3"
                    app:layout_constraintHorizontal_bias="0.067"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.266"
                    tools:src="@drawable/img_menu" />

                <ImageView
                    android:id="@+id/imageImage1"
                    imageRes="@{@drawable/img_image1}"
                    android:layout_width="318dp"
                    android:layout_height="353dp"
                    android:layout_marginBottom="12dp"
                    android:scaleType="fitXY"
                    app:layout_constraintBottom_toTopOf="@+id/imageZoomIn"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.494"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:src="@drawable/img_image1" />

                <ImageView
                    android:id="@+id/imageZoomIn"
                    imageRes="@{@drawable/img_zoomin}"
                    android:layout_width="@dimen/_65pxh"
                    android:layout_height="@dimen/_52pxh"
                    android:clickable="true"
                    android:scaleType="fitXY"
                    app:layout_constraintBottom_toTopOf="@+id/imageGroup37"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.926"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:src="@drawable/img_zoomin" />

                <ImageView
                    android:id="@+id/imageView26"
                    android:layout_width="56dp"
                    android:layout_height="30dp"
                    android:clickable="true"
                    app:layout_constraintEnd_toStartOf="@+id/imageView27"
                    app:layout_constraintHorizontal_bias="0.321"
                    app:layout_constraintStart_toStartOf="@+id/imageGroup37"
                    app:layout_constraintTop_toTopOf="@+id/imageView27"
                    app:srcCompat="@drawable/img_group_1" />

                <ImageView
                    android:id="@+id/imageView27"
                    android:layout_width="56dp"
                    android:layout_height="30dp"
                    android:clickable="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintStart_toStartOf="@+id/imageGroup37"
                    app:layout_constraintTop_toTopOf="@+id/imageGroup37"
                    app:layout_constraintVertical_bias="0.555"
                    app:srcCompat="@drawable/img_group_12" />

                <ImageView
                    android:id="@+id/imageView28"
                    android:layout_width="65dp"
                    android:layout_height="32dp"
                    android:clickable="true"
                    app:layout_constraintEnd_toEndOf="@+id/imageGroup37"
                    app:layout_constraintHorizontal_bias="0.704"
                    app:layout_constraintStart_toEndOf="@+id/imageView27"
                    app:layout_constraintTop_toTopOf="@+id/imageView27"
                    app:srcCompat="@drawable/img_group_10" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
