                    android:layout_width="245dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="8dp"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:inputType="text"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingBottom="@dimen/_3pxv"
                    android:singleLine="true"
                    android:text="@{editarPerfilVM.editarPerfilModel.txtTelfono1}"
                    app:layout_constraintBottom_toTopOf="@+id/txtBirthdayEditarPerfil"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txtEmailEditarPerfil2"
                    app:layout_constraintVertical_bias="0.333"
                    tools:text="Telefono" />

                <TextView
                    android:id="@+id/txtBirthdayEditarPerfil"
                    style="@style/txtSolidRounded_1"
                    android:layout_width="245dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="40dp"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:inputType="text"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingBottom="@dimen/_3pxv"
                    android:singleLine="true"
