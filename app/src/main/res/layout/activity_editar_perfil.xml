                <ImageView
                    android:id="@+id/imageInterfaceAdd2"
                    android:layout_width="@dimen/_52pxh"
                    android:layout_height="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_163pxh"
                    android:layout_marginTop="52dp"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_interfaceadd2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.72"
                    app:layout_constraintStart_toStartOf="@+id/imageGroup"
                    app:layout_constraintTop_toBottomOf="@+id/constraintGroup160" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintGroup160"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginEnd="@dimen/_4pxh"
                    app:layout_constraintBottom_toTopOf="@+id/imageInterfaceAdd2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imagePngkey2"
                        imageRes="@{@drawable/img_pngkey2}"
                        android:layout_width="@dimen/_73pxh"
                        android:layout_height="@dimen/_76pxh"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.87"
                        app:layout_constraintStart_toEndOf="@+id/imageMenu"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:src="@drawable/img_pngkey2" />

                    <ImageView
                        android:id="@+id/imageMenu"
                        imageRes="@{@drawable/img_menu}"
                        android:layout_width="@dimen/_64pxh"
                        android:layout_height="@dimen/_61pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_170pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/imagePngkey2"
                        app:layout_constraintHorizontal_bias="0.0"
