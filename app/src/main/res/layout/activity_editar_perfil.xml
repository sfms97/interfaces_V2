                    app:layout_constraintHorizontal_bias="0.502"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageGroup"
                    tools:text="Username" />

                <TextView
                    android:id="@+id/txtEmailEditarPerfil2"
                    style="@style/txtSolidRounded_1"
                    android:layout_width="245dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="8dp"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:inputType="text"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingBottom="@dimen/_3pxv"
                    android:singleLine="true"
                    android:text="@{editarPerfilVM.editarPerfilModel.txtTelfono1}"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.497"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txtLastname1"
                    tools:text="Email" />

                <TextView
                    android:id="@+id/txtEditar1"
                    style="@style/txtSansitaSwashedromanregular18"
                    android:layout_width="99dp"
                    android:layout_height="32dp"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@{editarPerfilVM.editarPerfilModel.txtEditar1}"
                    app:layout_constraintBottom_toBottomOf="@+id/viewRectangle2"
                    app:layout_constraintEnd_toEndOf="@+id/viewRectangle2"
                    app:layout_constraintStart_toStartOf="@+id/viewRectangle2"
                    app:layout_constraintTop_toTopOf="@+id/viewRectangle2"
                    tools:text="@string/lbl_editar" />

                <TextView
                    android:id="@+id/txtLastname1"
                    style="@style/txtSolidRounded_1"
                    android:layout_width="245dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="12dp"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:inputType="text"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingBottom="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@{editarPerfilVM.editarPerfilModel.txtLastname1}"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.497"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txtNombreEditarPerfil"
                    tools:text="Apellidos" />

                <TextView
                    android:id="@+id/txtNombreEditarPerfil"
