                    app:layout_constraintEnd_toEndOf="@+id/imageGroup19"
                    app:layout_constraintHorizontal_bias="0.646"
                    app:layout_constraintStart_toEndOf="@+id/imageView15"
                    app:srcCompat="@drawable/img_group_10" />

