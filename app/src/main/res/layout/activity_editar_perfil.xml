                    tools:text="Fecha de Nacimiento" />

                <TextView
                    android:id="@+id/txtUsernameEditarPerfil"
                    style="@style/txtSolidRounded_1"
                    android:layout_width="245dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="48dp"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:inputType="text"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingBottom="@dimen/_3pxv"
                    android:singleLine="true"
                    android:text="@{editarPerfilVM.editarPerfilModel.txtTelfono1}"
