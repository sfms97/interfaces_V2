                        android:clickable="true"
                        android:id="@+id/viewRectangle20"
                        android:layout_height="@dimen/_29pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_28pxh"
